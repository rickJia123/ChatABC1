<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <item android:state_checked="false">        <shape android:shape="rectangle">            <size android:width="20dp" android:height="20dp" />            <corners android:radius="20dp" />            <stroke android:color="@color/color_EEEEEE" android:width="1dp" />            <solid android:color="@color/white" />        </shape>    </item>    <item android:state_checked="true">        <shape android:shape="rectangle">            <size android:width="20dp" android:height="20dp" />            <corners android:radius="20dp" />            <stroke android:color="@color/color_EEEEEE" android:width="1dp" />            <solid android:color="@color/white" />        </shape>    </item></selector>